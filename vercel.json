{
    "routes": [
      { "src": "^/assets/(.*)", "dest": "/assets/$1" },
      { "src": "^/favicon.ico", "dest": "/favicon.ico" },
      { "src": "/(.*)", "dest": "/index.html" }
    ],
    "rewrites": [
      { "source": "/(.*)", "destination": "/" }
    ],
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite"
  }