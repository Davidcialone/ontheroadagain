{
  "builds": [
    {
      "src": "client/src/main.jsx",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client/dist"  // Chemin correct vers le dossier de sortie
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "public": true
}
