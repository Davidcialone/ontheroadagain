{
    "name": "on-the-road-again",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "workspaces": [
    "client",
    "api"
    ],
    "scripts": {
    "install-all": "npm install && cd client && npm install && cd ../api && npm install", 
    "build": "npm run build-client && npm run build-api",
    "build-client": "cd client && npm run build", 
    "build-api": "echo 'No build step needed for API'", 
    "lint": "cd client && npm run lint",
    "preview": "cd client && npm run preview", 
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:api\"",
    "dev:client": "cd client && vite --mode development", 
    "dev:api": "dotenv -e api/.env.local -- node api/index.js",
    "start": "concurrently \"npm run start:client\" \"npm run start:api\"",
    "start:client": "cd client && vite --mode production", 
    "start:api": "dotenv -e api/.env.production -- node api/index.js" 
    },

    "devDependencies": {
        "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
        "@types/jwt-decode": "^3.1.0",
        "@vitejs/plugin-react": "^4.3.2",
        "concurrently": "^6.5.1",
        "dotenv-cli": "^7.4.2",
        "gh-pages": "^6.1.1",
        "vite": "^5.4.11",
        "vite-plugin-static-copy": "^2.0.0"
    },
    "dependencies": {
        "@emotion/react": "^11.13.3",
        "@emotion/styled": "^11.13.0",
        "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
        "@mui/icons-material": "^6.1.6",
        "@mui/material": "^6.1.6",
        "@react-google-maps/api": "^2.20.3",
        "bcrypt": "^5.1.1",
        "bootstrap": "^5.3.3",
        "buffer": "^6.0.3",
        "cloudinary": "^1.21.0",
        "compressorjs": "^1.2.1",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "email-validator": "^2.0.4",
        "exif-js": "^2.3.0",
        "express": "^4.21.1",
        "framer-motion": "^11.11.9",
        "google-map-react": "^2.2.1",
        "inherits": "^2.0.4",
        "joi": "^17.13.3",
        "js-cookie": "^3.0.5",
        "jsonwebtoken": "^9.0.2",
        "jwt-decode": "^4.0.0",
        "leaflet": "^1.9.4",
        "multer": "^1.4.5-lts.1",
        "multer-storage-cloudinary": "^4.0.0",
        "node-fetch": "^3.3.2",
        "pg": "^8.13.0",
        "piexif": "^0.0.2",
        "process": "^0.11.10",
        "prop-types": "^15.8.1",
        "react-datepicker": "^7.4.0",
        "react-dom": "^18.3.1",
        "react-leaflet": "^4.2.1",
        "react-player": "^2.16.0",
        "react-responsive-carousel": "^3.2.23",
        "react-router-dom": "^6.27.0",
        "react-slick": "^0.30.2",
        "react-stars": "^2.2.5",
        "sequelize": "^6.37.4",
        "slick-carousel": "^1.8.1",
        "stream-browserify": "^3.0.0",
        "util": "^0.12.5",
        "vite-plugin-node-polyfills": "^0.2.0",
        "yet-another-react-lightbox": "^3.21.6"
    },
    "proxy": "http://localhost:5000"
}
