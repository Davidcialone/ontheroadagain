{
    "builds": [
      {
        "src": "client/package.json", 
        "use": "@vercel/static-build",
        "config": {
          "distDir": "client/dist" 
        }
      }
    ],
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  